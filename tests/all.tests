def add_glsl1(name):
	testname = 'glsl1-' + name
	glean[testname] = GleanTest('glsl1')
	glean[testname].env['PIGLIT_TEST'] = name
execfile(os.path.dirname(__file__) + '/glean-glsl1.tests')

