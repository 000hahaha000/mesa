def add_glslparsertest(shader, result):
	glslparsertest[shader] = PlainExecTest(['glslparsertest', 'tests/glslparsertest/shaders/' + shader, result])
def add_otherglslparsertest(shader, result, min_glsl_version = '1.10'):
	glslparsertest[shader] = PlainExecTest(['glslparsertest', 'tests/glslparsertest/' + shader, result, min_glsl_version])
execfile(os.path.dirname(__file__) + '/glslparser.tests')

