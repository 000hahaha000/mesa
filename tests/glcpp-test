#!/bin/sh
set -e

echo "Caution: These results are just verifying parse-ability, not correctness!"

for test in *.c; do
    echo "Testing $test"
    ../glcpp < $test > $test.out
    gcc -E $test -o $test.gcc
    grep -v '^#' < $test.gcc > $test.expected
#    diff -B -u $test.expected $test.out
done
