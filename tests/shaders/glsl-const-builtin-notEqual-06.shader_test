
[require]
GLSL >= 1.20

[vertex shader file]
glsl-mvp.vert

[fragment shader]
void main()
{
  const bvec3 res = notEqual(ivec3(0, 8, 89),
                             ivec3(4, -7, 33));
  gl_FragColor = (res == bvec3(true, true, true))
    ? vec4(0.0, 1.0, 0.0, 1.0) : vec4(1.0, 0.0, 0.0, 1.0);
}

[test]
draw rect -1 -1 2 2
probe all rgb 0.0 1.0 0.0
