[require]
GL >= 2.0
GLSL >= 1.20

[vertex shader file]
glsl-mvp.vert

[fragment shader]
#version 120
void main()
{
    const float x = floor(-0.5);
    const float y = floor(0.0);
    const float z = floor(0.5);
    gl_FragColor = vec4(x + 1.7, y, z, 1.0);
}

[test]
clear color 0.0 0.0 0.0 0.0
clear
ortho
draw rect 10 10 10 10
probe rgb 15 15 0.7 0.0 0.0
