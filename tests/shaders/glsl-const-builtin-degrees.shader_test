[require]
GLSL >= 1.20

[vertex shader file]
glsl-mvp.vert

[fragment shader]
void main() {
   const int x = int(degrees(3.1415926535));
   if (x == 180)
      gl_FragColor = vec4(0.0, 1.0, 0.0, 1.0);
   else
      gl_FragColor = vec4(1.0, 0.0, 0.0, 1.0);
}

[test]
clear color 0.0 0.0 0.0 0.0
clear
ortho
draw rect 10 10 10 10
probe rgb 15 15 0.0 1.0 0.0
