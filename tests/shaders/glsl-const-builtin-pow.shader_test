[require]
GL >= 2.0
GLSL >= 1.20

[vertex shader file]
glsl-mvp.vert

[fragment shader]
#version 120
void main()
{
    const vec2 v = pow(vec2(3,65), vec2(2,0.5));
    const float splat = v.x - v.y;
    gl_FragColor = vec4(0.0, splat, 0.0, 1.0);
}

[test]
clear color 0.0 0.0 0.0 0.0
clear
ortho
draw rect 10 10 10 10
probe rgb 15 15 0.0 0.937742251701451 0.0
