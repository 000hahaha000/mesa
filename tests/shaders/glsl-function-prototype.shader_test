[require]
GL >= 2.0
GLSL >= 1.10

[vertex shader]
void main()
{
	gl_Position = gl_Vertex;
}

[fragment shader]

// Function prototype:
float half(float x);

void main()
{
	float c = half(0.8);
	gl_FragColor = vec4(c);
}

float half(float x)
{
	return 0.5 * x;
}


