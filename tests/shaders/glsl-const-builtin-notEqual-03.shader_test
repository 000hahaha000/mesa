
[require]
GLSL >= 1.20

[vertex shader file]
glsl-mvp.vert

[fragment shader]
void main()
{
  const bvec3 res = notEqual(vec3(13.4, -0.9, 12.55),
                             vec3(13.4, 12.0, -55.3));
  gl_FragColor = (res == bvec3(false, true, true))
    ? vec4(0.0, 1.0, 0.0, 1.0) : vec4(1.0, 0.0, 0.0, 1.0);
}

[test]
draw rect -1 -1 2 2
probe all rgb 0.0 1.0 0.0
